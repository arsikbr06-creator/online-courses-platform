# Переменные окружения для backend

# Порт сервера
PORT=3000

# Окружение
NODE_ENV=production

# База данных PostgreSQL
DB_HOST=postgres
DB_PORT=5432
DB_NAME=online_courses
DB_USER=postgres
DB_PASSWORD=your_secure_password_here

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Secret
JWT_SECRET=your_very_secure_jwt_secret_change_in_production

# Cors origins (разделенные запятыми)
CORS_ORIGINS=http://localhost,http://localhost:80
